{
  "filters": {
    "comments": true
  },
  "rules": {
    "preset-ja-spacing": {
      "ja-space-between-half-and-full-width": {
        "space": "always"
      },
      "ja-space-around-code": {
        "before": true,
        "after": true
      }
    },
    "preset-ja-technical-writing": false,
    "spellcheck-tech-word": false
  }
}
